

.container .bg {
    height: 100%;
    width: 100%;
    position: absolute;
}
.container .content {
  position: relative;
  width: 100%;
  height: 100%;
  padding-top: 120rpx;
  /* background: url('../images/1_5.png') no-repeat; */
  background-size: 100% 100%;
  box-sizing: border-box;
}
.container .content .header {
  display: block;
  margin: 0 auto;
}
.container .content .login-out {
  position: absolute;
  top: 30rpx;
  right: 20rpx;
  z-index: 99;
  width: 158rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  font-size: 28rpx;
  color: #FE7F1C;
  background: #fff;
  border-radius: 40rpx;
}
.container .content .nav-list {
  position: absolute;
  left: 0;
  bottom: 100rpx;
  z-index: 1;
  display: flex;
  width: 100%;
}
.container .content .nav-list .nav-item {
  flex: 1;
}
.container .content .nav-list .nav-item .nav-icon {
  display: block;
  margin: 0 auto;
  max-height: 136rpx;
  max-width: 136rpx;
}
.container .content .nav-list .nav-item .nav-text {
  margin-top: 16rpx;
  font-size: 24rpx;
  color: #666;
  line-height: 26rpx;
  text-align: center;
}
